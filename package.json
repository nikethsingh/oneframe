{
  "name": "oneframe",
  "version": "1.0.0",
  "description": "testing oneframe",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1 && echo $name",
    "build": "webpack --config=webpack.config.js && babel ./lib/index.js -d ./lib --config-file=./.babelrc",
    "semantic-release": "semantic-release",
    "cz": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikethsingh/oneframe.git"
  },
  "author": "zedak",
  "license": "ISC",
  "files": ["lib/*"],
  "bugs": {
    "url": "https://github.com/nikethsingh/oneframe/issues"
  },
  "homepage": "https://github.com/nikethsingh/oneframe#readme",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-preset-es2015": "^6.24.1",
    "commitizen": "^4.2.4",
    "semantic-release": "^17.4.3",
    "styled-components": "^5.3.0",
    "terser-webpack-plugin": "^4.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "copy-webpack-plugin": "^6.4.1",
    "cz-conventional-changelog": "^3.2.0",
    "glob": "^7.1.7",
    "regenerator-runtime": "^0.13.7",
    "semantic-release": "^17.4.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0"
  },
  "config": {
    "private": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "release": {
      "prepare": [
        "@semantic-release/changelog",
        "@semantic-release/npm",
        {
          "path": "@semantic-release/git",
          "assets": [
            "package.json",
            "package-lock.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
